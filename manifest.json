{
	"manifest_version": 3,
	"name": "get a life",
	"author": "Dhruva Srinivas & Shine Chang",
	"version": "0.0.0",
	"description": "helping you (yes you), to get a life.",
	"action": {
		"default_popup": "popup.html"
	},
	"background": {
		"service_worker": "background.js"
	},
	"permissions": [
		"tabs",
		"activeTab",
		"scripting",
		"storage"
	],
	"host_permissions": [
		"http://*/*",
		"https://*/*",
		"<all_urls>"
	]
}
