{
	"manifest_version": 2,
	"name": "get a life",
	"author": "Dhruva Srinivas & Shine Chang",
	"version": "0.0.0",
	"description": "helping you (yes you), to get a life.",
	"background": {
		"scripts": [
			"background.js"
		]
	},
	"content_scripts": [
		{
			"matches": [
				"http://*/*",
				"https://*/*",
				"<all_urls>"
			],
			"js": [
				"content.js"
			]
		}
	],
	"permissions": [
		"tabs",
		"https://*/*"
	]
}
